{
  "name": "feast-finder",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npx convex dev --once && concurrently -r npm:dev:web npm:dev:convex",
    "dev:web": "vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "npx convex dev",
    "build": "vite build && tsc --noEmit",
    "start": "netlify dev",
    "format": "prettier --write .",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.67",
    "@ai-sdk/openai-compatible": "^1.0.27",
    "@auth/core": "^0.37.4",
    "@chakra-ui/react": "^3.29.0",
    "@convex-dev/auth": "^0.0.90",
    "@convex-dev/geospatial": "^0.1.9",
    "@convex-dev/react-query": "^0.0.0-alpha.11",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mendable/firecrawl-js": "^4.5.0",
    "@sentry/tanstackstart-react": "^10.23.0",
    "@sentry/vite-plugin": "^4.6.0",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.132.2",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.132.2",
    "@types/leaflet": "^1.9.21",
    "ai": "^5.0.93",
    "convex": "^1.28.0",
    "framer-motion": "^12.23.24",
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "^1.0.0",
    "@netlify/vite-plugin-tanstack-start": "^1.1.11",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/eslint-config": "^0.3.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "~9.2.1",
    "netlify-cli": "^23.10.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
